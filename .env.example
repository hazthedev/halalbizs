# NextAuth URL (Your Vercel deployment URL)
NEXTAUTH_URL=https://your-project.vercel.app

# Image domain (Your Bagisto backend domain)
IMAGE_DOMAIN=demo.halalbizs.com

# Bagisto store domain (Your Bagisto backend URL with https://)
BAGISTO_STORE_DOMAIN=https://demo.halalbizs.com

# Bagisto session key (optional - uses default "bagisto_session" if empty)
BAGISTO_SESSION=

# Company information
COMPANY_NAME=Halalbizs
SITE_NAME=Halalbizs

# Twitter info
TWITTER_CREATOR=
TWITTER_SITE=

# Revalidation duration (for caching or ISR in seconds)
REVALIDATION_DURATION=3600

# NextAuth secret (Generate with: openssl rand -base64 32)
NEXTAUTH_SECRET=
